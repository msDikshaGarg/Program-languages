(define (countCodes word)
	(if(string-null? word)(+ 0)
		(if(string-null? (getCountryFromCode(string-head word 2))) (countCodes(string-tail word 2))
		(+ 1 (countCodes(string-tail word 2)))
		)
	)
)

(define (sumDigits alp)
	(if(string-null? alp)(+ 0 0)
		(if(string->number(string-head alp 1))(+(string->number(string-head alp 1))(sumDigits(string-tail alp 1)))
		(+(sumDigits(string-tail alp 1)))
		)
	)
)

;Making tuples from two lists

(define(ltuples val list2)
 (if (null? list2)
  '()
  (cons(cons val (car list2))(ltuples val (cdr list2)))
 )
)
  
(define (caltuples list1 list2)
 (if(null? list1)
   '()
   (append(ltuples(car list1)list2)(caltuples(cdr list1)list2))
 )
)