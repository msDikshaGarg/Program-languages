; Implementation of Fibonacci series 
(define (fib n)
 (define (closed)
  (define (enclosed ans prev c)
   (if (<= c 0)
    ans (enclosed prev (+ ans prev)(- c 1))
   )
  )
  (enclosed 1 1 n)
 )
 closed
)

